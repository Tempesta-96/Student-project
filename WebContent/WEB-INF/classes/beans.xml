<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="sign" class="bean.SignInBean">
	</bean>

	<bean id="register" class="bean.RegisterBean">
	</bean>

	<bean id="add" class="bean.AddBean">
	</bean>

	<bean id="delete" class="bean.DeleteBean">
	</bean>

	<bean id="list" class="bean.ListBean">
	</bean>

	<bean id="Update" class="bean.UpdateBean">
	</bean>



</beans>
